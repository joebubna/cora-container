{
  "name": "cora/collection",
  "description": "Keep collections of data with flexible access methods.",
  "keywords": ["collection", "array", "list", "group", "cora", "demo", "php"],
  "license": "MIT",
  "authors": [
    {
      "name": "Josiah Bubna"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "phpunit/phpunit": "7.*"
  },
  "config": {
    "vendor-dir": "app/vendor"
  },
  "autoload": {
    "psr-4": {
      "Cora\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "app/tests/",
      "Classes\\": "app/classes/"
    }
  }
}